{
  "name": "superinbox-core",
  "version": "0.1.0",
  "description": "SuperInbox Core Backend - A unified entry point for digital information with AI-powered intelligent routing",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts",
    "db:migrate": "tsx src/storage/migrations/run.ts",
    "db:seed": "tsx src/storage/seeds/run.ts"
  },
  "keywords": [
    "inbox",
    "ai",
    "automation",
    "routing",
    "productivity"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "better-sqlite3": "^9.2.2",
    "dotenv": "^16.3.1",
    "zod": "^3.22.4",
    "uuid": "^9.0.1",
    "axios": "^1.6.5",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.5",
    "@types/better-sqlite3": "^7.6.9",
    "@types/uuid": "^9.0.7",
    "@types/cors": "^2.8.17",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "tsx": "^4.7.0",
    "vitest": "^1.2.1",
    "@vitest/coverage-v8": "^1.2.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
