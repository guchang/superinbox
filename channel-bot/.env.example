# Service Configuration
PORT=3002
NODE_ENV=development

# SuperInbox Core API
CORE_API_URL=http://localhost:3001/v1
# Enabled channels: telegram,lark,wework
ENABLED_CHANNELS=telegram,lark

# Database (User Mapping)
DATABASE_PATH=./data/channel-bot.db

# Telegram (if enabled)
TELEGRAM_BOT_TOKEN=your-telegram-bot-token

# Lark (if enabled)
LARK_APP_ID=your-lark-app-id
LARK_APP_SECRET=your-lark-app-secret
# Optional in socket mode; required in callback mode for event verification
LARK_VERIFICATION_TOKEN=
