# Channel Bot runtime
NODE_ENV=production
PORT=3002

# Core API (recommended to use public domain for independent deployment)
CORE_API_URL=https://www.superinbox.top/v1
CORE_API_KEY=replace_with_channel_bot_api_key

# Enabled channels: telegram,lark,wework
ENABLED_CHANNELS=telegram,lark

# Channel bot local db path in container
DATABASE_PATH=/app/data/channel-bot.db

# Telegram
TELEGRAM_BOT_TOKEN=

# Lark (socket long connection mode)
LARK_APP_ID=
LARK_APP_SECRET=
LARK_VERIFICATION_TOKEN=
LARK_ENCRYPT_KEY=

# Logging
LOG_LEVEL=info
LOG_FORMAT=json
